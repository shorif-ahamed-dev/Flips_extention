{
  "manifest_version": 3,
  "name": "Flips - Fifa SignUp Extension",
  "version": "1.0",
  "description": "Autofill the form with a single click.",
  "permissions": ["scripting", "activeTab", "tabs"],
  "action": {
    "default_title": "Fill Form",
    "default_icon": {
      "16": "rocket.png",
      "48": "rocket.png",
      "128": "rocket.png"
    }
  },
  "icons": {
    "16": "rocket.png",
    "48": "rocket.png",
    "128": "rocket.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://localhost:3000/*"]
}
